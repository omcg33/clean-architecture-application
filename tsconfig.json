{
    "compilerOptions": {
        "sourceMap": true,
        "removeComments": true,

        "declaration": true,
        "strict": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "skipLibCheck": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true
    }
}